import random

writing_group_list = ["Sin√©ad", "Jessica", "Laura", "Vanessa",
                      "David", "Arike", "Joe", "Alix","Hasson",
                      "Abhirup", "Rachael", "Blanche", "Donald",
                      "Lu", "Terry", "Bev","Weronika"]
rand_list = []
for i in range(len(writing_group_list)):
    rand_int = random.randint(0, len(writing_group_list)-1)
    rand_writing_group_member = writing_group_list[rand_int]
    if rand_writing_group_member not in rand_list:
        rand_list.append(rand_writing_group_member)

for name in rand_list:
    print(name)
